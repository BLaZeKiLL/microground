{
  "version": 1,
  "projects": {
    "wc-fabric-core": {
      "root": "libs/wc-fabric/core",
      "sourceRoot": "libs/wc-fabric/core/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/wc-fabric/core/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/wc-fabric/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/wc-fabric/core",
            "tsConfig": "libs/wc-fabric/core/tsconfig.lib.json",
            "packageJson": "libs/wc-fabric/core/package.json",
            "main": "libs/wc-fabric/core/src/index.ts",
            "assets": ["libs/wc-fabric/core/*.md"]
          }
        }
      }
    },
    "wc-fabric-angular": {
      "projectType": "library",
      "root": "libs/wc-fabric/angular",
      "sourceRoot": "libs/wc-fabric/angular/src",
      "prefix": "microground",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/wc-fabric/angular/tsconfig.lib.json",
            "project": "libs/wc-fabric/angular/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/wc-fabric/angular/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/wc-fabric/angular/src/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/wc-fabric/angular/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    },
    "samples-webcomponents": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/samples/webcomponents",
      "sourceRoot": "apps/samples/webcomponents/src",
      "prefix": "microground",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/samples/webcomponents",
            "index": "apps/samples/webcomponents/src/index.html",
            "main": "apps/samples/webcomponents/src/main.ts",
            "polyfills": "apps/samples/webcomponents/src/polyfills.ts",
            "tsConfig": "apps/samples/webcomponents/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/samples/webcomponents/src/favicon.ico",
              "apps/samples/webcomponents/src/assets"
            ],
            "styles": ["apps/samples/webcomponents/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/samples/webcomponents/src/environments/environment.ts",
                  "with": "apps/samples/webcomponents/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "samples-webcomponents:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "samples-webcomponents:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "samples-webcomponents:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/samples/webcomponents/src/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/samples/webcomponents/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "samples-webcomponents-e2e": {
      "root": "apps/samples/webcomponents-e2e",
      "sourceRoot": "apps/samples/webcomponents-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/samples/webcomponents-e2e/cypress.json",
            "tsConfig": "apps/samples/webcomponents-e2e/tsconfig.e2e.json",
            "devServerTarget": "samples-webcomponents:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "samples-webcomponents:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/samples/webcomponents-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  }
}
